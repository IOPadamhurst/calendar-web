{
    "homepage": "https://cal.openintents.org",
    "name": "oi-calendar",
    "author": "friedger@gmail.com",
    "version": "0.6.4",
    "private": true,
    "dependencies": {
        "blockstack": "18.2.1",
        "ical.js": "^1.3.0",
        "ics": "^2.10.0",
        "matrix-js-sdk": "^0.14.3",
        "moment": "^2.22.1",
        "react": "^16.3.2",
        "react-big-calendar": "^0.20.3",
        "react-bootstrap": "^0.32.1",
        "react-datetime": "^2.14.0",
        "react-dom": "^16.3.2",
        "react-pdf": "^3.0.4",
        "react-redux": "^5.0.7",
        "react-scripts": "2.1.3",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "REACT_APP_VERSION=$(node -pe 'require(\"./package.json\").version') react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "testflow": "mocha spec/*.spec.js --compilers js:babel-core/register --presets @babel/preset-stage-2",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "lint": "eslint ./src ./spec",
        "lint:fix": "eslint --fix ./src ./spec"
    },
    "devDependencies": {
        "@babel/cli": "7.2.3",
        "@babel/core": "7.2.2",
        "@babel/node": "7.2.2",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-stage-2": "7.0.0",
        "@babel/register": "7.0.0",
        "babel-polyfill": "6.26.0",
        "babel-preset-es2015": "6.24.1",
        "eslint-config-prettier": "^4.0.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-config-standard-react": "^7.0.2",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-promise": "^4.0.1",
        "eslint-plugin-react": "^7.12.4",
        "eslint-plugin-standard": "^4.0.0",
        "gh-pages": "^1.1.0",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.3",
        "mocha": "5.2.0",
        "prettier": "^1.16.4",
        "prop-types": "^15.6.2"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "eslintConfig": {
        "extends": "react-app"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/*.js": [
            "npm run lint:fix",
            "git add"
        ]
    }
}
